            if (debug_scenes) print "[Scene 'Tener sed' ends]^";
            if (GProperty(43, 2, p57_recurring)) scene_status-->1 = 0; else scene_status-->1 = 2; scene_ended-->1 = the_time;
            scene_endings-->1 = (scene_endings-->1)|2;
            scene_latest_ending-->1 = 1;
            if (debug_scenes) print "[Scene 'Tener sed' ends happily]^";
            if (GProperty(43, 2, p57_recurring)) scene_status-->1 = 0; else scene_status-->1 = 2; FollowRulebook(382);
            scene_ended-->1 = the_time;
            scene_endings-->1 = (scene_endings-->1)|4;
            scene_latest_ending-->1 = 2;
            FollowRulebook(381);
            FollowRulebook(WHEN_SCENE_ENDS_RB, 2);
            jump CScene;
        }
